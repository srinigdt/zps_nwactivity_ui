{  
   "_version":"1.1.0",
   "sap.app":{  
      "_version":"1.1.0",
      "id":"gdt.ui.ps.networkcomp",
      "type":"application",
      "i18n":"i18n/i18n.properties",
      "title":"{{appTitle}}",
      "description":"{{appDescription}}",
      "applicationVersion":{  
         "version":"1.0.0"
      },
      "dataSources":{  
         "MaterialOrderingService":{  
            "uri":"/sap/opu/odata/sap/Z_PS_NETWORK_COMP_ADD_SRV",
            "type":"OData",
            "settings":{  
               "odataVersion":"2.0"
            }
         }
      }
   },
   "sap.ui":{  
      "_version":"1.1.0",
      "technology":"UI5",
      "deviceTypes":{  
         "desktop":true,
         "tablet":true,
         "phone":true
      },
      "supportedThemes":[  
         "sap_bluecrystal"
      ],
      "fullWidth":true,
      "contentDensities":{  
         "compact":true,
         "cozy":true
      }
   },
   "sap.ui5":{  
      "_version":"1.1.0",
      "rootView":"gdt.ui.ps.networkcomp.views.App",
      "dependencies":{  
         "minUI5Version":"1.30",
         "libs":{  
            "sap.m":{  

            }
         }
      },
      "models":{  
         "i18n":{  
            "type":"sap.ui.model.resource.ResourceModel",
            "uri":"i18n/i18n.properties"
         }
      },
      "routing":{  
         "config":{  
            "routerClass":"sap.m.routing.Router",
            "viewType":"XML",
            "viewPath":"gdt.ui.ps.networkcomp.views",
            "controlId":"app",
            "controlAggregation":"pages",
            "transition":"slide",
            "bypassed":{  
               "target":"notFound"
            }
         },
         "routes":[  
            {  
               "pattern":"",
               "name":"home",
               "target":"home"
            },
            {  
               "pattern":"detail/{params}/:tab:",
               "name":"detail",
               "target":"detail"
            }
         ],
         "targets":{  
            "home":{  
               "viewName":"Selection",
               "viewLevel":1
            },
            "detail":{  
               "viewName":"Details",
               "viewLevel":2
            },
            "notFound":{  
               "viewName":"NotFound",
               "transition":"show",
               "viewLevel":2
            }
         }
      },
      "resources":{  
         "css":[  
            {  
               "uri":"css/style.css"
            }
         ]
      }
   }
}